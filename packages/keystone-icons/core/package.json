{
  "name": "@fe-infra/keystone-icons",
  "description": "A Keystone icon library package for web and stencil applications.",
  "version": "0.7.2",
  "license": "MIT",
  "bugs": "https://code.byted.org/ad/byted-web-components/issues",
  "repository": {
    "type": "git",
    "url": "https://code.byted.org/ad/byted-web-components",
    "directory": "keystone-icons/core"
  },
  "keywords": [
    "Keystone",
    "Icons",
    "Icon",
    "SVG"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "unpkg": "dist/keystone-icons/keystone-icons.esm.js",
  "files": [
    "dist/",
    "loader/",
    "components/"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist loader",
    "build": "run-s clean build:*",
    "build:icons": "tsx scripts/build-icons/index.ts",
    "build:stencil": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "echo \"stencil test --spec --e2e\"",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "4.38.0"
  },
  "devDependencies": {
    "@byted-keystone/vue-output-target": "workspace:*",
    "@bytedance-dev/rollup-plugin-dealwith-stencil-custom-elements": "workspace:*",
    "@stencil/react-output-target": "^0.5.3",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "~4.17.12",
    "cheerio": "^1.0.0-rc.12",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lodash-es": "~4.17.21",
    "puppeteer": "^22.12.1",
    "rimraf": "^5.0.1"
  }
}