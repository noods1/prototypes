{
  "name": "@fe-infra/keystone",
  "description": "An out-of-the-box design system for beautifully crafted web components.",
  "version": "0.19.24",
  "license": "MIT",
  "bugs": "https://code.byted.org/ad/byted-web-components/issues",
  "repository": {
    "type": "git",
    "url": "https://code.byted.org/ad/byted-web-components.git",
    "directory": "keystone/core"
  },
  "keywords": [
    "Keystone",
    "Web Components",
    "Design System"
  ],
  "sideEffects": [
    "./dist/components/index.js",
    "./dist/esm/loader.js",
    "./dist/cjs/loader.js",
    "./dist/esm-es5/loader.js",
    "./dist/keystone/keystone.css"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/components/index.js",
  "exports": {
    ".": {
      "import": "./dist/components/index.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/components": {
      "import": "./dist/components/index.js"
    },
    "./dist/components/*": {
      "import": "./dist/components/*"
    },
    "./dist/keystone/keystone.css": "./dist/keystone/keystone.css",
    "./dist/keystone/*": {
      "import": "./dist/keystone/*"
    },
    "./dist/cjs/*": {
      "require": "./dist/cjs/*"
    },
    "./dist/esm/*": {
      "import": "./dist/esm/*"
    },
    "./dist/types/*": "./dist/types/*",
    "./dist/types/entities": "./dist/types/entities/index.d.ts"
  },
  "types": "dist/types/index.d.ts",
  "style": "dist/keystone/keystone.css",
  "unpkg": "latest",
  "unpkgFiles": [
    "dist/keystone"
  ],
  "files": [
    "dist/",
    "loader/",
    "LICENSE",
    "readme.md",
    "postinstall.js"
  ],
  "scripts": {
    "build": "run-s clean build:*",
    "build:icons": "node ./scripts/build-icons-into-globalscript.mjs",
    "build:stencil": "stencil build --docs",
    "build:entities": "tsx ./scripts/build-entity-types.mts",
    "start": "run-s clean start:*",
    "start:stencil": "stencil build --docs --watch",
    "test": "run-p test:spec test:e2e",
    "test:spec": "NODE_ENV=test stencil test --spec",
    "test:spec:coverage": "NODE_ENV=test stencil test --spec --coverage",
    "test:e2e": "NODE_ENV=test playwright test",
    "test:e2e:debug": "NODE_ENV=test playwright test --ui",
    "test:e2e:update-snapshots": "NODE_ENV=test playwright test --update-snapshots='changed'",
    "generate": "stencil generate",
    "clean": "rimraf dist loader docsreadme www",
    "postinstall": "node postinstall.js",
    "inject": "props-inject -p packages/keystone-ui/core/src/components && prettier --write src/components"
  },
  "dependencies": {
    "@fe-infra/keystone-design-tokens": "workspace:^*",
    "@fe-infra/keystone-icons": "workspace:*",
    "@fe-infra/keystone-locales": "workspace:*",
    "@floating-ui/dom": "1.6.6",
    "@oddbird/popover-polyfill": "^0.5.2",
    "@stencil/core": "4.38.0",
    "@stencil/store": "2.2.0",
    "@tanstack/virtual-core": "^3.13.12",
    "animejs": "~3.2.1",
    "async-validator": "~4.2.5",
    "classnames": "~2.5.1",
    "date-fns": "~3.6.0",
    "dayjs": "1.11.13",
    "lodash-es": "~4.17.21",
    "loglevel": "1.9.2",
    "node-fetch": "^2.7.0",
    "sortablejs": "1.15.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.10.1",
    "@byted-keystone/react-output-target": "workspace:*",
    "@byted-keystone/vue-output-target": "workspace:*",
    "@bytedance-dev/rollup-plugin-dealwith-stencil-custom-elements": "workspace:*",
    "@rollup/plugin-image": "~3.0.3",
    "@rollup/plugin-typescript": "^11.0.0",
    "@stencil-community/eslint-plugin": "^0.10.0",
    "@stencil-community/postcss": "~2.2.0",
    "@stencil/playwright": "0.2.1",
    "@stencil/sass": "^3.0.12",
    "@types/animejs": "~3.1.12",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "~4.17.12",
    "@types/sortablejs": "1.15.8",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "css-tree": "~2.3.1",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lodash": "^4.17.21",
    "postcss-scss": "^4.0.6",
    "postcss-url": "~10.1.3",
    "prettier": "^3.8.0",
    "rimraf": "^5.0.1",
    "sass": "~1.77.6",
    "stencil-inline-svg": "~1.1.0",
    "tslib": "^2.5.0",
    "typescript": "^5.9.2",
    "workbox-build": "~7.1.1"
  }
}