{
  "version": 2,
  "buildCommand": "npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/adpreview-multishow",
      "destination": "/adpreview-multishow/index.html"
    },
    {
      "source": "/adpreview-multishow/:path*",
      "destination": "/adpreview-multishow/index.html"
    },
    {
      "source": "/catalog-creation-proto",
      "destination": "/catalog-creation-proto/index.html"
    },
    {
      "source": "/catalog-creation-proto/:path*",
      "destination": "/catalog-creation-proto/index.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/((?!adpreview-multishow|catalog-creation-proto|assets|.*\\.(js|css|woff|woff2|png|jpg|jpeg|gif|svg|mp4|webp)).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/adpreview-multishow/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
