{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/adpreview-multishow/:path*",
      "destination": "https://adpreview-multishow.vercel.app/:path*"
    },
    {
      "source": "/catalog-creation-proto/:path*",
      "destination": "https://catalog-creation-proto.vercel.app/:path*"
    },
    {
      "source": "/react-tailwind-app/:path*",
      "destination": "https://react-tailwind-app.vercel.app/:path*"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}
