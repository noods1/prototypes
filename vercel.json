{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/adpreview-multishow",
      "destination": "/adpreview-multishow/index.html"
    },
    {
      "source": "/adpreview-multishow/(.*)",
      "destination": "/adpreview-multishow/index.html"
    },
    {
      "source": "/catalog-creation-proto",
      "destination": "/catalog-creation-proto/index.html"
    },
    {
      "source": "/catalog-creation-proto/(.*)",
      "destination": "/catalog-creation-proto/index.html"
    },
    {
      "source": "/react-tailwind-app",
      "destination": "/react-tailwind-app/index.html"
    },
    {
      "source": "/react-tailwind-app/(.*)",
      "destination": "/react-tailwind-app/index.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/((?!adpreview-multishow|catalog-creation-proto|react-tailwind-app).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/adpreview-multishow/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/react-tailwind-app/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
