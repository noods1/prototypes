{
  "version": 2,
  "buildCommand": "corepack enable && corepack prepare pnpm@10.21.0 --activate && pnpm install --no-frozen-lockfile && pnpm run build",
  "outputDirectory": "dist",
  "devCommand": "pnpm run dev",
  "installCommand": "corepack enable && corepack prepare pnpm@10.21.0 --activate && pnpm install --no-frozen-lockfile",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/adpreview-multishow",
      "destination": "/adpreview-multishow/index.html"
    },
    {
      "source": "/adpreview-multishow/:path*",
      "destination": "/adpreview-multishow/index.html"
    },
    {
      "source": "/catalog-creation-proto",
      "destination": "/catalog-creation-proto/index.html"
    },
    {
      "source": "/catalog-creation-proto/:path*",
      "destination": "/catalog-creation-proto/index.html"
    },
    {
      "source": "/media_crop_catalog_manager",
      "destination": "/media_crop_catalog_manager/index.html"
    },
    {
      "source": "/media_crop_catalog_manager/:path*",
      "destination": "/media_crop_catalog_manager/index.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/adpreview-multishow/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
